import time
import RPi.GPIO as gpio
gpio.setmode(gpio.BOARD)
gpio.setup(19, gpio.OUT)
gpio.output(19, True)
time.sleep(0.5)
gpio.output(19, False)
time.sleep(0.5)
gpio.output(19, True)
time.sleep(0.5)
gpio.output(19, False)
gpio.output(19, True)
time.sleep(0.4)
gpio.output(19, False)
# Run in Raspberry Pi control LED connect to pin 19, which is not GPIO 19
#
